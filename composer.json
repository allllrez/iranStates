{
    "name": "alrez/iran-states",
    "description": "A package for managing provinces and cities of Iran in Laravel.",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,

    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5|^11.0",
        "orchestra/testbench": "^8.5|^9.0"
    },

    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alrez\\IranStates\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alrez\\IranStates\\IranStatesServiceProvider"
            ]
        }
    },
    "authors": [{
        "name": "alrez",
        "email": "anabestanireza@yahoo.com"
    }],
    "keywords": [
        "laravel",
        "php",
        "iran",
        "iran-states",
        "iran-cities",
        "laravel-12",
        "php-8.2",
        "iran-provinces",
        "iran-package"
    ]
}