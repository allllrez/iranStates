{
    "name": "alrez/iran_provinces",
    "description": "A package for managing provinces and cities of Iran in Laravel.",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.0",

    "require": {
        "laravel/framework": "^10.0|^11.0",
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },

    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alrez\\IranProvinces\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alrez\\IranProvinces\\IranProvincesServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "alrez",
            "email": "anabestanireza@yahoo.com"
        }
    ],
    "keywords": [
        "laravel",
        "php",
        "iran",
        "iran-states",
        "iran-cities"
    ],
    "autoload-dev": {
        "psr-4": {
            "Alrez\\IranProvinces\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}