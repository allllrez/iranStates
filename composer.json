{
    "name": "alrez/iran_provinces",
    "description": "A package for managing provinces and cities of Iran in Laravel.",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,

    "require": {
        "laravel/framework": "^10.0|^11.0",
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.9"
    },

    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alrez\\IranProvinces\\": "src/",
            "Alrez\\IranProvinces\\Database\\": "src/database/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [{
        "name": "alrez",
        "email": "anabestanireza@yahoo.com"
    }],
    "keywords": [
        "laravel",
        "php",
        "iran",
        "iran-states",
        "iran-cities"
    ]
}